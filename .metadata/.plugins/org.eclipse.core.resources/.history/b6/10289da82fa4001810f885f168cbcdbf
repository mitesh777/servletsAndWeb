<body>
	
	<div>One</div>
	<div>two</div>
	<div>three</div>
	<div>four</div>
	<div>five</div>
	
	<button id="hide">Hide</button>
	<button id="show">Show</button>
	<script src="../../js/jquery-3.3.1.min.js"></script>
	<script type="text/javascript">
	
	$(function(){
		$("#hide").click(function()){
			$("div").hide();
		};
		$("#show").click(function()){
			$("div").show();
		};
	})
	function hide(){
		var divs = document.getElementsByTagName("div");
		//document.querySelectorAll("div");
		$("div").hide();
	}
	
	function show(){
		$("div").show();
	}
	</script>
</body>
