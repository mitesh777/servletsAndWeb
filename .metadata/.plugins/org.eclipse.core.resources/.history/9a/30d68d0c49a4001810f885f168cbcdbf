	<script type="text/javascript">
	
	$(function(){
		var friendsList = ['dan','kar','sham'];
		$('#getFriends').click(function(){
			$.each(friendsList, function(i,x){
				console.log(x);
				$("#friends").append('<li>')
			}})
		});
		
	});
	
	</script>
