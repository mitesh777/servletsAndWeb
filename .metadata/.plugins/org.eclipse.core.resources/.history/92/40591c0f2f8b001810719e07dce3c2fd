
public class ProjectManager extends Employee{
	private double performanceBonus;
	private double projectCommission;
	public double getPerformanceBonus() {
		return performanceBonus;
	}
	public void setPerformanceBonus(double performanceBonus) {
		this.performanceBonus = performanceBonus;
	}
	public double getGetPerformanceBonus() {
		return getPerformanceBonus;
	}
	public void setGetPerformanceBonus(double getPerformanceBonus) {
		this.getPerformanceBonus = getPerformanceBonus;
	}
	@Override
	double getMonthlySalary() {
		ManagerSalaryCalculator msc = new ManagerSalaryCalculator();
		return msc.calculateManagerSalary(this);
		 //teamLeadSalary;
		// TODO Auto-generated method stub
		//return 0;
	}
	
}
