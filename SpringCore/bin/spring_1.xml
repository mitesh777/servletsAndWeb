<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://www.springframework.org/schema/context 
        http://www.springframework.org/schema/context/spring-context-3.0.xsd"
    xmlns:context="http://www.springframework"    >

	<bean id="employee" class="di.example_1.Employee">
		<property name="employeeId" value="1" />
		<property name="employeeName" value="Chandra" />
		<property name="salary" value="7500"></property>
	</bean>
	
	<bean id="employee2" class="di.example_1.Employee">
		<property name="employeeId" value="2" />
		<property name="employeeName" value="Chandra2" />
		<property name="salary" value="75002"></property>
	</bean>
</beans>